{
  "version": 1,
  "paths": {
    "/invitecontact": {
      "permissions": {
        "setting": "private",
        "auth": [
          "read",
          "update"
        ]
      },
      "lambdaFunction": "ltsEmailInvites"
    },
    "/connect": {
      "name": "/connect",
      "lambdaFunction": "extConnector",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read"
        ],
        "guest": [
          "create",
          "read"
        ]
      }
    }
  }
}